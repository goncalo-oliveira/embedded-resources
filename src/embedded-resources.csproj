<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>System.Reflection</RootNamespace>
    <AssemblyName>faactory.extensions.embedded</AssemblyName>
    <PackageId>Faactory.Extensions.EmbeddedResources</PackageId>
    <Version>0.1</Version>
    <Authors>Goncalo Oliveira</Authors>
    <Description>Assembly extensions for embedded resources</Description>
    <RepositoryUrl>https://github.com/goncalo-oliveira/embedded-resources</RepositoryUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageIcon>faactory.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="../faactory.png" Pack="true" PackagePath="/"/>
    <None Include="../README.md" Pack="true" PackagePath="/"/>
  </ItemGroup>

</Project>
